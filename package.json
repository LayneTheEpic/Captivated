{
	"name": "captivated",
	"version": "1.1.0",
	"description": "A simple, no bullshit keylogger.",
	"main": "main.js",
	"scripts": {
		"start": "electron .",
		"pack": "electron-builder --dir",
		"dist": "electron-builder"
	},
	"author": "Layne Ogden",
	"license": "MIT",
	"dependencies": {
		"fs-extra": "^9.0.1",
		"iohook": "^0.7.2"
	},
	"devDependencies": {
		"electron": "^11.1.0",
		"electron-builder": "22.9.1"
	},
	"build": {
		"appId": "Captivated",
		"mac": {
			"category": "Developer Tools"
		},
		"extraFiles": [
			"json",
			"src",
			"window"
		]
	},
	"iohook": {
		"targets": [
			"node-72",
			"electron-85"
		],
		"platforms": [
			"win32"
		],
		"arches": [
			"x64"
		]
	}
}
